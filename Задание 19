#include <iostream>
using namespace std;

int** memory_allocate(const int& n, const int& m)
{
	int** a;
	a = new int* [n];
	for (int i = 0; i < n; i++)
		a[i] = new int[m];
	return a;
}

int** input(int** a, int n, int m)
{
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
			scanf_s("%d", &a[i][j]);
	return a;
}

int destroy(int** a, int n, int m)
{
	for (int i = 0; i < n; i++)
		delete[] a[i];
	return 0;
}

int countstr(int** a, int n, int m)
{
	int count = 0;
	for (int i = 0; i < n; i++)
	{
		bool flag = true;
		for (int j = 0; j < m; j++)
		{
			for (int k = 0; k < m; k++)
			{
				if (k != j)
					if (a[i][j] == a[i][k])
						flag = false;
			}
		}
		if (flag == true)
			count++;
	}
	return count;
}

int countcolumn(int** a, int n, int m)
{
	int count = 0;
	for (int i = 0; i < m; i++)
	{
		bool flag = true;
		for (int j = 0; j < n; j++)
		{
			for (int k = 0; k < n; k++)
			{
				if (k != j)
					if (a[j][i] == a[k][i])
						flag = false;
			}
		}
		if (flag == true)
			count++;
	}
	return count;
}

int main()
{
	int** a;
	int n,m;
	scanf_s("%d%d", &n, &m);
	a = memory_allocate(n,m);
	input(a, n, m);
	int countst = countstr(a, n, m);
	int countcl = countcolumn(a, n, n);
	printf("%d\n%d\n", countst, countcl);

	destroy(a, n,m);

	return 0;
}
