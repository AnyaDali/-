#include<iostream>
using namespace std;

const int n = 5;

int* mem(const int& n)
{
	int* arr = new int[n];
	return arr;
}

void vvod(int* arr)
{
	for (int i = 0; i < n; i++)
	{
		scanf_s("%d", &arr[i]);
	}
}

int sum(const int* arr)
{
	int res = 0;
	int ymn = 1;
	
	for (int i = 0; i < n; i++)
	{
		if (arr[i] < 0)
			break;
		ymn *= arr[i];
		res += ymn;
	}
	return res;
}

int main()
{
	int* arr = mem(n);
	vvod(arr);
	int res = sum(arr);
	printf_s("%d", res);
	delete[] arr;
	return 0;
}
