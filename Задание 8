#include <stdio.h>
using namespace std;
//Дано натуральное число n. Определить, является ли простым числом
int main() 
{
    int n, count;
    count = 0;
    scanf("%d", &n);
    int del = 2;
    while( del < n)
    }
        if(count != 0)
            break;
        if(n % del == 0)
            count = count + 1;
        del = del + 1;
    }

    if(count == 0)
        printf("Число является простым\n);
    else
        printf("Число не является простым\n);
    return 0;
}
